<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.example.sirjackovich.c196">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name=".TermsActivity"
      android:label="@string/terms_text" />
    <activity
      android:name=".CoursesActivity"
      android:label="@string/courses_text" />
    <activity
      android:name=".MentorsActivity"
      android:label="@string/mentors_text" />
    <activity
      android:name=".AssessmentsActivity"
      android:label="@string/assessments_text"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".AssessmentsDetailsActivity"
      android:label="@string/assessment_details" />
  </application>

</manifest>